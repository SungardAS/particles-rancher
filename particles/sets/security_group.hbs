{{
  resource "m:vpc" "security_group"
  logicalId="SecurityGroup"
  groupDescription="Rancher Server"
}}

{{
  resource "m:vpc" "security_group/ingress"
  logicalId="RancherServerIngress"
  groupId=(ref "SecurityGroup")
  sourceSecurityGroupId=(ref "SecurityGroup")
  fromPort=9345
  toPort=9345
}}
